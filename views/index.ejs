<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% for(var i =0 ; i<messages.length ;i++){ %>
      <div>
        <%= messages[i].text %>
      </div>
      <div>
        <%= messages[i].user %>

      </div>
      <div>
        <%= messages[i].added %>

      </div>
      <% } %>
  </body>
</html>
